images  = |Atlas|
fonts   = |Fonts|
tooltip	= { id = segoeui color = 0xFF00FFEF size = { x = 15 y = 15 } thresh = { x = 0.12 y = 0.55 } }
	
buttons = 
[ 
	{	
		name		= button
		up			= { id = |pixel| color = 0xFF53FF3F }
		down		= { id = |pixel| color = 0xFF53321F }
		hl			= { id = |pixel| color = 0xFF53F232 }
		downHl		= { id = |pixel| color = 0xFF53aaaF }
		font		= { id = segoeui color = 0xFFFFF4EF size = { x = 15 y = 15 } thresh = { x = 0.12 y = 0.55 } }
		vertical	= center
		horizontal	= center
	}
	{	
		name		= button2
		up			= { id = |pixel| color = 0xFF53AA3F }
		down		= { id = |pixel| color = 0xFF53321F }
		hl			= { id = |pixel| color = 0xFF53F232 }
		downHl		= { id = |pixel| color = 0xFF53aaaF }
		font		= { id = segoeui color = 0xFFFFF4EF size = { x = 15 y = 15 } thresh = { x = 0.12 y = 0.55 } }
		vertical	= center
		horizontal	= center
	}
